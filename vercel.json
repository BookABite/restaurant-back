{
    "builds": [
        {
            "src": "bookabite/config/asgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12",
                "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "bookabite/config/asgi.py:application"
        }
    ]
}