{
    "version": 2,
    "builds": [
      {
        "src": "bookabite/asgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "bookabite/asgi.py"
      }
    ]
  }